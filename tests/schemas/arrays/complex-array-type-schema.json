{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "title": "Complex-Array-Schema",
    "type": "object",
    "properties": {
        "complexArrayValue": {
            "type": "array",
            "items": [
                { "type": "string" },
                { "type": "number" },
                { "type": "boolean" },
                { "type": "object", "properties": { "name": { "type": "string" } } },
                { "type": "array", "items": { "type": "number" } }
            ],
            "description": "A complex array type"
        }
    }
}
